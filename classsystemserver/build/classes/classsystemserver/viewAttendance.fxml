<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.text.*?>
<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="544.0" stylesheets="@css/delete1.css" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="classsystemserver.model.AttendanceController">
    
     <children>
      <VBox prefHeight="400.0" prefWidth="532.0" stylesheets="@css/delete1.css">
         <children>
            <Label prefHeight="39.0" prefWidth="504.0" text="                        Class Attendance">
               <font>
                  <Font size="21.0" />
               </font>
               <VBox.margin>
                  <Insets left="10.0" right="10.0" top="5.0" />
               </VBox.margin>
            </Label>
            <TableView fx:id="table" prefHeight="294.0" prefWidth="495.0">
              <columns>
                <TableColumn fx:id="name" prefWidth="140.0" text="NAME" />
                <TableColumn fx:id="regno" prefWidth="165.0" text="Registration Number" />
                  <TableColumn fx:id="mark" prefWidth="226.0" text="Student Number" />
              </columns>
               <VBox.margin>
                  <Insets left="10.0" right="10.0" top="3.0" />
               </VBox.margin>
            </TableView>
            <HBox prefHeight="68.0" prefWidth="500.0">
               <children>
                  <Button mnemonicParsing="false" onAction="#takeAttendance" prefHeight="54.0" prefWidth="256.0" text="Take Attendance">
                     <padding>
                        <Insets bottom="3.0" left="3.0" top="3.0" />
                     </padding>
                     <HBox.margin>
                        <Insets bottom="3.0" right="3.0" top="3.0" />
                     </HBox.margin>
                  </Button>
                  <Button fx:id="btncancel" mnemonicParsing="false" onAction="#canclelButton" prefHeight="54.0" prefWidth="250.0" text="Cancel">
                     <HBox.margin>
                        <Insets bottom="3.0" left="3.0" top="3.0" />
                     </HBox.margin>
                  </Button>
               </children>
               <VBox.margin>
                  <Insets left="10.0" />
               </VBox.margin>
            </HBox>
         </children>
      </VBox>
   </children>
</AnchorPane>
